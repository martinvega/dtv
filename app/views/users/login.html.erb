<div id="login">
  <%= form_for @user, :url => create_session_users_path do |f| %>
    
    <div class="field">
      <p><%= image_tag 'directvLogo.jpg', :size => "160x130",
  :alt => "Bienvenido a Plataforma Multisat" %></p>
    </div>
    <div class="field">
      <%= f.label :user %>
      <%= f.text_field :user, :maxlength => 18, :autofocus => true %>
    </div>
    <div class="field">
      <%= f.label :password %>
      <%= f.password_field :password, :maxlength => 18 %>
    </div>
    
    <div class="field">
      <span><%= f.submit "Ingresar" %></span>
    </div>
  <% end %>
</div>